<?xml version="1.0" encoding="UTF-8"?>
<AUTOSAR xmlns="http://autosar.org/schema/r4.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://autosar.org/schema/r4.0 file//../../AUTOSAR_ARXML_Schema/AUTOSAR_00046.xsd">
	<AR-PACKAGES>
		<AR-PACKAGE>
			<SHORT-NAME>Runnables</SHORT-NAME>
			<ELEMENTS>
				<!-- Input Interfaces --> 
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SR_IF_Inputs</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>VAL1</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>VAL2</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">AUTOSAR_PlatformTypes/ImplementationDataTypes/uint16</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- Output Interfaces --> 
				<SENDER-RECEIVER-INTERFACE>
					<SHORT-NAME>SR_IF_Outputs</SHORT-NAME>
					<DATA-ELEMENTS>
						<VARIABLE-DATA-PROTOTYPE>
							<SHORT-NAME>Result</SHORT-NAME>
							<TYPE-TREF DEST="IMPLEMENTATION-DATA-TYPE">AUTOSAR_PlatformTypes/ImplementationDataTypes/uint32</TYPE-TREF>
						</VARIABLE-DATA-PROTOTYPE>
					</DATA-ELEMENTS>
				</SENDER-RECEIVER-INTERFACE>
				<!-- APP SWC -->
				<APPLICATION-SW-COMPONENT-TYPE>
					<SHORT-NAME>SWC1</SHORT-NAME>
					<!-- PORTS --> 
					<PORTS>
						<P-PORT-PROTOTYPE>
							<SHORT-NAME>PP_SR_sum</SHORT-NAME>
							<PROVIDED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SR_IF_Outputs</PROVIDED-INTERFACE-TREF>
						</P-PORT-PROTOTYPE>
						<R-PORT-PROTOTYPE>
							<SHORT-NAME>RP_SR_sum</SHORT-NAME>
							<REQUIRED-INTERFACE-TREF DEST="SENDER-RECEIVER-INTERFACE">/SR_IF_Inputs</REQUIRED-INTERFACE-TREF>
						</R-PORT-PROTOTYPE>					
					</PORTS>
									
					<!-- Internal Behaviour --> 
					<INTERNAL-BEHAVIORS>
						<SWC-INTERNAL-BEHAVIOR>
							<SHORT-NAME>SWC1_Behavior</SHORT-NAME>
								
							<!-- EVENTS --> 
							<EVENTS>
								<!-- Timing Event -->
								<TIMING-EVENT>
									<SHORT-NAME>TE_10ms</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">SWC1/SWC1_Behavior/Sum_Runnable</START-ON-EVENT-REF>
									<PERIOD>0.01</PERIOD>
								</TIMING-EVENT>
								
								<!-- Init Event --> 
								<INIT-EVENT>
									<SHORT-NAME>IE_Init</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">SWC1/SWC1_Behavior/Sum_Runnable</START-ON-EVENT-REF>								
								</INIT-EVENT>
								
								<!-- Data Received Event on VAL1 --> 
								<DATA-RECEIVED-EVENT>
									<SHORT-NAME>DRE_Received</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">SWC1/SWC1_Behavior/Sum_Runnable</START-ON-EVENT-REF>
									<DATA-IREF>
										<TARGET-DATA-ELEMENT-REF DEST="VARIABLE-DATA-PROTOTYPE">/SR_IF_Inputs/VAL1</TARGET-DATA-ELEMENT-REF>
									</DATA-IREF>
								</DATA-RECEIVED-EVENT>
								
								<!-- DATA Write Complete Event -->
								<DATA-WRITE-COMPLETED-EVENT>
									<SHORT-NAME>DWCE_DataWrite</SHORT-NAME>
									<START-ON-EVENT-REF DEST="RUNNABLE-ENTITY">SWC1/SWC1_Behavior/Sum_Runnable</START-ON-EVENT-REF>
									<EVENT-SOURCE-REF DEST="VARIABLE-ACCESS">/SR_IF_Outputs/Result</EVENT-SOURCE-REF>
								</DATA-WRITE-COMPLETED-EVENT>
							
							</EVENTS>
							
							
							
							<!-- Runnables --> 
							<RUNNABLES>
							<RUNNABLE-ENTITY>
								<SHORT-NAME>Sum_Runnable</SHORT-NAME>
								<MINIMUM-START-INTERVAL>0.0</MINIMUM-START-INTERVAL>
								<CAN-BE-INVOKED-CONCURRENTLY>false</CAN-BE-INVOKED-CONCURRENTLY>
								<!-- Read Access --> 
								<DATA-READ-ACCESSS>
									<VARIABLE-ACCESS>
										<SHORT-NAME>Data_val1</SHORT-NAME>
										<ACCESSED-VARIABLE>
											<AUTOSAR-VARIABLE-IREF>
												<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/SWC1/RP_SR_sum</PORT-PROTOTYPE-REF>
												<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/SR_IF_Inputs/VAL1</TARGET-DATA-PROTOTYPE-REF>
											</AUTOSAR-VARIABLE-IREF>
										</ACCESSED-VARIABLE>
									</VARIABLE-ACCESS>
									<VARIABLE-ACCESS>
										<SHORT-NAME>Data_val2</SHORT-NAME>
										<ACCESSED-VARIABLE>
											<AUTOSAR-VARIABLE-IREF>
												<PORT-PROTOTYPE-REF DEST="R-PORT-PROTOTYPE">/SWC1/RP_SR_sum</PORT-PROTOTYPE-REF>
												<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/SR_IF_Inputs/VAL2</TARGET-DATA-PROTOTYPE-REF>
											</AUTOSAR-VARIABLE-IREF>
										</ACCESSED-VARIABLE>
									</VARIABLE-ACCESS>
								</DATA-READ-ACCESSS>
								<!-- Write access -->
								<DATA-WRITE-ACCESSS>
									<VARIABLE-ACCESS>
										<SHORT-NAME>Data_SumResult</SHORT-NAME>
										<ACCESSED-VARIABLE>
											<AUTOSAR-VARIABLE-IREF>
												<PORT-PROTOTYPE-REF DEST="P-PORT-PROTOTYPE">/SWC1/PP_SR_sum</PORT-PROTOTYPE-REF>
												<TARGET-DATA-PROTOTYPE-REF DEST="VARIABLE-DATA-PROTOTYPE">/SR_IF_Outputs/Result</TARGET-DATA-PROTOTYPE-REF>
											</AUTOSAR-VARIABLE-IREF>
										</ACCESSED-VARIABLE>
									</VARIABLE-ACCESS>
								</DATA-WRITE-ACCESSS>
								
								<!-- Symbol --> 
								<SYMBOL>Set_CMD</SYMBOL>
							</RUNNABLE-ENTITY>
								
							</RUNNABLES>
						</SWC-INTERNAL-BEHAVIOR>
							
					</INTERNAL-BEHAVIORS>
				
				</APPLICATION-SW-COMPONENT-TYPE>

			
								
			
			</ELEMENTS>
			
		</AR-PACKAGE>
	</AR-PACKAGES>
</AUTOSAR>